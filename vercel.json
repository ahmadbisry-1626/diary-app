{
    "version": 2,
    "env": {
        "DATABASE_URL": "@DATABASE_URL",
        "jwtSecretKey": "@jwtSecretKey",
        "jwtRefreshTokenKey": "@jwtRefreshTokenKey"
    },
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/main.js",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "PATCH",
                "OPTIONS"
            ]
        }
    ]
}